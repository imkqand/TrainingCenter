﻿
@model IEnumerable<Cuorse>
@{
    ViewData["Title"] = "Index";
    // layout="~/Views/Shared/_Layout2.cshtml";
}

<h1>Cuorse List</h1>
@* <partial name="~/_NotificationsPartial"/> *@
<div class="text-end">
    <a asp-action="Create" class="btn btn-primary mb-3">Create New</a>
</div>

<table class="table table-striped mb-3">

    <thead>
        <tr>
            <th>#</th>
            <th>CuorseName</th>
            <th>CuorseDescription</th>
            <th>CuoresHouers</th>
            <th>StudentId</th>
            <th>LectuerId</th>

            <th>Action</th>
        </tr>
    </thead>
    <tbody>

        @foreach (var emp in Model)
        {
            <tr>
                <td>@emp.CuorseId</td>
                <td>@emp.CuorseName</td>
                <td>@emp.CuorseDescription</td>
                <td>@emp.CuoresHouers</td>
                <td>@emp.StudentId</td>
                <td>@emp.LectuerId</td>
                <td>
                    <a asp-action="Edit" asp-route-Id="@emp.CuorseId" class="btn btn-warning">Edit</a>
                    <a asp-action="Delete" asp-route-Id="@emp.CuorseId" class="btn btn-danger">Delete</a>
                </td>
            </tr>

        }



    </tbody>


</table>
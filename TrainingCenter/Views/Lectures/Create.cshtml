@model Lectures
@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>


<form asp-action="Create" method="post">




    <div>
        <label>  Name:</label>
        <input asp-for="Name" name="Name" class="form-control" />
    </div>

    <div>
        <label>  Department   :</label>
        <input asp-for="Department" name="Department" class="form-control" />
    </div>

    <div>
        <label>    Phone  :</label>
        <input asp-for="Phone" name="Phone" class="form-control" />
    </div>

    <div>
        <label>    Course Code  :</label>
        <input asp-for="CourseCode" name="CourseCode" class="form-control" />
    </div>






    <hr>
    <button type="submit" class="btn btn-primary">Save</button>


</form>